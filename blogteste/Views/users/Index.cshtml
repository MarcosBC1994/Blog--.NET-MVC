@model IEnumerable<ApplicationUser>

<h2>Utilizadores</h2>

<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a asp-action="Details" asp-route-id="@user.Id">Detalhes</a>
                </td>
            </tr>
        }
    </tbody>
</table>

